(function (modules) {
  function require (id) {
    const module = {
      exports: {}
    }

    const [ fn, mapping ] = modules[id]

    function localRequire (filePath) {
      const id = mapping[filePath]
      return require(id)
    }

    fn(localRequire, module, module.exports)

    return module.exports
  }

  require(0)

})({
  <% grapgh.forEach(item => { %>
    '<%- item.id %>': [
      function(require, module, exports) {
        <%- item.code %>
      },
      <%- JSON.stringify(item['mapping']) %>
    ],
  <% }) %>
})